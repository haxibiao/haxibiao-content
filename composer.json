{
    "name": "haxibiao/content",
    "description": "哈希表内容系统",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ivan",
            "email": "ivan@haxibiao.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "laravel/framework": "^8.0",
        "nuwave/lighthouse": "^4.10",
        "haxibiao/console": "*",
        "haxibiao/helpers": "*",
        "haxibiao/config": "*",
        "haxibiao/matomo": "*",
        "haxibiao/cms": "*",
        "haxibiao/question": "*",
        "haxibiao/sns": "*",
        "haxibiao/media": "*"
    },
    "autoload": {
        "psr-4": {
            "Haxibiao\\Content\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Haxibiao\\Content\\Tests\\": "tests/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "repositories": {},
    "extra": {
        "laravel": {
            "providers": [
                "Haxibiao\\Content\\ContentServiceProvider"
            ]
        }
    }
}
