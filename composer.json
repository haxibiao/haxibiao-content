{
    "name": "haxibiao/content",
    "description": "哈希表内容系统",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ivan",
            "email": "ivan@haxibiao.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "laravel/framework": "^8.0",
        "haxibiao/lighthouse": "^5.0",
        "haxibiao/console": "*",
        "haxibiao/helpers": "*",
        "haxibiao/config": "*",
        "haxibiao/matomo": "*",
        "haxibiao/cms": "*",
        "haxibiao/question": "*",
        "haxibiao/sns": "*",
        "haxibiao/media": "*"
    },
    "autoload": {
        "psr-4": {
            "Haxibiao\\Content\\": "src/"
        },
        "classmap": [
            "database/seeders/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "database/factories/",
            "tests/"
        ]
    },
    "repositories": {},
    "extra": {
        "laravel": {
            "providers": [
                "Haxibiao\\Content\\ContentServiceProvider"
            ]
        }
    }
}
