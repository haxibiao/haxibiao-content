{
    "name": "haxibiao/content",
    "type": "library",
    "require": {
        "php": ">=7.3",
        "haxibiao/helpers": "*",
        "haxibiao/config": "*",
        "haxibiao/matomo": "*",
        "Haxibiao/cms": "*",
        "Haxibiao/sns": "*",
        "haxibiao/media": "*"
    },
    "autoload": {
        "psr-4": {
            "Haxibiao\\Content\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Haxibiao\\Content\\Tests\\": "tests/"
        }
    },
    "repositories": {
        "helpers": {
            "type": "vcs",
            "url": "https://github.com/haxibiao/haxibiao-helpers.git"
        },
        "base": {
            "type": "vcs",
            "url": "https://github.com/haxibiao/haxibiao-base.git"
        },
        "config": {
            "type": "vcs",
            "url": "https://github.com/haxibiao/haxibiao-config.git"
        },
        "matomo": {
            "type": "vcs",
            "url": "https://github.com/haxibiao/haxibiao-matomo.git"
        },
        "media": {
            "type": "vcs",
            "url": "https://github.com/haxibiao/haxibiao-media.git"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Haxibiao\\Content\\ContentServiceProvider"
            ]
        }
    }
}
